                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <!-- Enable autorun by setting this property to 'true' -->
            <meta-data
                android:name="autorun"
                android:value="false" />
            <!--
            Add your test or test suite's class name here or remove the
            following element to make <package-name>.AllTests
            the class to be run as test.
            -->
            <meta-data
